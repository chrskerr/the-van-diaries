{
	"name": "kc-freelance-sveltekit",
	"version": "0.0.1",
	"scripts": {
		"dev": "svelte-kit dev",
		"build": "svelte-kit build",
		"postbuild": "npx purgecss --css $(pwd)/build/css/*.css --content $(pwd)/build/**/*.html $(pwd)/build/**/*.js --output $(pwd)/build/css && npx css-minify -d build/css -o build/css && mv $(pwd)/build/css/main.min.css $(pwd)/build/main.css",
		"start": "svelte-kit start"
	},
	"devDependencies": {
		"@sveltejs/adapter-static": "1.0.0-next.3",
		"@sveltejs/kit": "1.0.0-next.53",
		"css-minify": "^1.1.0",
		"eslint": "^7.22.0",
		"purgecss": "^4.0.2",
		"sass": "^1.32.8",
		"svelte": "^3.29.0",
		"vite": "^2.0.4",
		"vite-plugin-markdown": "^2.0.2"
	},
	"type": "module",
	"engines": {
		"node": ">= 12.17.0"
	},
	"dependencies": {
		"date-fns": "^2.19.0",
		"front-matter": "^4.0.2",
		"leaflet": "^1.7.1",
		"leaflet-geosearch": "^3.2.2",
		"leaflet-providers": "^1.11.0",
		"leaflet.locatecontrol": "^0.73.0",
		"lodash": "^4.17.21",
		"marked": "^2.0.1"
	},
	"license": "MIT",
	"browserslist": "defaultS"
}
